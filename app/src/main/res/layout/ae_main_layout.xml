<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    tools:context=".launch.AEMainActivity">


    <android.support.v7.widget.CardView
        android:id="@+id/entry_card_1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/entry_tv_height"
        card_view:cardUseCompatPadding="true"
        card_view:cardCornerRadius="4dp"
        android:clickable="true"
        android:foreground="@drawable/developing_item_card_bg"
        >

        <TextView
            android:id="@+id/entry_tv_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center"
            android:text="test"
            android:textSize="@dimen/entry_tv_text_size"
            android:textColor="@color/entry_tv_text_color"
            />

    </android.support.v7.widget.CardView>

</RelativeLayout>
